<div class="container">
  <br>

  <div class="col-md-4">
    <%= form_with(url: managers_path, method: :get) do |f| %>
      <%= f.text_field :query, class: 'search-bar', placeholder: 'Search by Name, Artist, Email or Genres' %>
      <%= f.submit "Search", class: 'btn btn-secondary' %>
    <% end %>
  </div>
  <br>

  <table id="auditions" class="table table-bordered table-striped table-hover table-color">
    <caption>List of Auditions</caption>
    <%= render partial: 'audition_table' %>
  </table>


<br>
